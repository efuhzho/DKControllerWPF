                        <!--#region StopACSource -->
                        <GroupBox Grid.Row="0"
                                  Header="交流源停止命令"
                                  Margin="10"
                                  Background="AliceBlue">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <StackPanel Grid.Row="0"
