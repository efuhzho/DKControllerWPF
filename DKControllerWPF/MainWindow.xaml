                        <!--#region SetDisplayPage -->
                        <GroupBox Grid.Row="4"
                                  Header="设置当前显示页面"
                                  Margin="10"
                                  Background="AliceBlue">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <StackPanel Grid.Row="0"
                                            Orientation="Horizontal"
                                            Margin="10">
