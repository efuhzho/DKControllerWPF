                               VerticalContentAlignment="Center"
                               HorizontalContentAlignment="Center"
                               FontSize="18" />
                        <Label Grid.Row="11"
                               Grid.Column="2"
                               Grid.ColumnSpan="2"
                               Margin="2,2,2,2"
                               Padding="5"
                               Background="White"
                               Name="lbFreq"
                               VerticalContentAlignment="Center"
                               HorizontalContentAlignment="Center"
                               FontSize="18" />
                        <Label Grid.Row="11"
                               Grid.Column="4"
                               Margin="2,2,2,2"
                               Padding="5"
                               Background="LightGray"
                               Name="lbFreqC"
                               VerticalContentAlignment="Center"
                               HorizontalContentAlignment="Center"
                               FontSize="18" />
                    </Grid>
                    <GroupBox Grid.Row="0"
                              Grid.Column="0"
                              Header="读取交流源数据"
                              Margin="10"
                              Background="AliceBlue">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <StackPanel Grid.Row="0"
                                        Orientation="Horizontal"
                                        Margin="10">
                                <Button Content="ReadACSourceData"
                                        Padding="10,5"
                                        Margin="5"
                                        Click="Button_Click_ReadACSourceData" />
                                <Button Content="STOP Reading"
                                        Background="LightPink"
                                        Padding="10,5"
                                        Margin="5"
                                        Click="Button_Click_StopReadACSourceData" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal"
                                        Margin="10"
                                        Grid.Row="1">
                                <TextBlock Text="IsSuccess:"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           Margin="10,5,0,5"
                                           Padding="5" />
                                <TextBox x:Name="tbxReadACSourceDataIsSuccess"
                                         VerticalContentAlignment="Center"
                                         Margin="5"
                                         Padding="5" />
                                <TextBlock Text="ErrorCode:"
                                           Margin="10,5,0,5"
                                           VerticalAlignment="Center" />
                                <TextBox Name="tbxReadACSourceDataErrorCode"
                                         VerticalContentAlignment="Center"
                                         Margin="5"
                                         Padding="5" />
                                <TextBlock Text="Message:"
                                           Margin="10,5,0,5"
                                           VerticalAlignment="Center" />
                                <TextBox Name="tbxReadACSourceDataMesseage"
                                         VerticalContentAlignment="Center"
                                         Margin="5"
                                         Padding="5" />
                            </StackPanel>
                            <GroupBox Grid.Row="2"
                                      Header="设置交流源幅度"
                                      Margin="10"
                                      Background="LightGray">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <StackPanel Grid.Row="0"
                                                Orientation="Horizontal"
                                                Margin="10">
                                        <Button Content="WriteACSourceAmplitude"
                                                Padding="10,5"
                                                Margin="5"
                                                Click="Button_Click_WriteACSourceAmplitude" />
                                        <TextBlock Text="IsSuccess:"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   Margin="10,5,0,5"
                                                   Padding="5" />
                                        <TextBox x:Name="tbxWriteACSourceAmplitudeIsSuccess2"
                                                 VerticalContentAlignment="Center"
                                                 Margin="5"
                                                 Padding="5"
                                                 Text="{Binding ElementName=tbxWriteACSourceAmplitudeIsSuccess, Path=Text}" />
                                        <TextBlock Text="ErrorCode:"
                                                   Margin="10,5,0,5"
                                                   VerticalAlignment="Center" />
                                        <TextBox Name="tbxWriteACSourceAmplitudeErrorCode2"
                                                 VerticalContentAlignment="Center"
                                                 Margin="5"
                                                 Padding="5"
                                                 Text="{Binding ElementName=tbxWriteACSourceAmplitudeErrorCode, Path=Text}" />
                                        <TextBlock Text="Message:"
                                                   Margin="10,5,0,5"
                                                   VerticalAlignment="Center" />
                                        <TextBox Name="tbxWriteACSourceAmplitudeMesseage2"
                                                 VerticalContentAlignment="Center"
                                                 Margin="5"
                                                 Text="{Binding ElementName=tbxWriteACSourceAmplitudeMesseage, Path=Text}"
                                                 Padding="5" />
                                    </StackPanel>
                                    <StackPanel Grid.Row="1">
                                        <TextBlock Text="Content:下位机回复的原始报文"
                                                   Margin="10,5"
                                                   Padding="5" />
                                        <TextBox x:Name="tbxWriteACSourceAmplitudeResponse2"
                                                 Margin="10,5"
                                                 Padding="10"
                                                 
                                                 TextWrapping="WrapWithOverflow"
                                                 Text="{Binding ElementName=tbxWriteACSourceAmplitudeResponse, Path=Text}" />
                                    </StackPanel>
                                    <UniformGrid Grid.Row="2">
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Margin="10,10,0,10"
                                                       Padding="5"
                                                       Text="Ua:"
                                                       VerticalAlignment="Center"
                                                       HorizontalAlignment="Center" />
                                            <TextBox x:Name="txbAmplitudeUa2"
                                                     MinWidth="80"
                                                     Margin="0,10,10,10"
                                                     Padding="5"
                                                     Text="{Binding ElementName=txbAmplitudeUa, Path=Text}" />
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Margin="10,10,0,10"
                                                       Padding="5"
                                                       Text="Ub:"
                                                       VerticalAlignment="Center"
                                                       HorizontalAlignment="Center" />
                                            <TextBox x:Name="txbAmplitudeUb2"
                                                     MinWidth="80"
                                                     Margin="0,10,10,10"
                                                     Padding="5"
                                                     Text="{Binding ElementName=txbAmplitudeUb, Path=Text}" />
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Margin="10,10,0,10"
                                                       Padding="5"
                                                       Text="Uc:"
                                                       VerticalAlignment="Center"
                                                       HorizontalAlignment="Center" />
                                            <TextBox x:Name="txbAmplitudeUc2"
                                                     MinWidth="80"
                                                     Margin="0,10,10,10"
                                                     Padding="5"
                                                     Text="{Binding ElementName=txbAmplitudeUc, Path=Text}" />
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Margin="10,10,0,10"
                                                       Padding="5"
                                                       Text="Ia:"
                                                       VerticalAlignment="Center"
                                                       HorizontalAlignment="Center" />
                                            <TextBox x:Name="txbAmplitudeIa2"
                                                     MinWidth="80"
                                                     Margin="0,10,10,10"
                                                     Padding="5"
                                                     Text="{Binding ElementName=txbAmplitudeIa, Path=Text}" />
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Margin="10,10,0,10"
                                                       Padding="5"
                                                       Text="Ib:"
                                                       VerticalAlignment="Center"
                                                       HorizontalAlignment="Center" />
                                            <TextBox x:Name="txbAmplitudeIb2"
                                                     MinWidth="80"
                                                     Margin="0,10,10,10"
                                                     Padding="5"
                                                     Text="{Binding ElementName=txbAmplitudeIb, Path=Text}" />
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Margin="10,10,0,10"
                                                       Padding="5"
                                                       Text="Ic:"
                                                       VerticalAlignment="Center"
                                                       HorizontalAlignment="Center" />
                                            <TextBox x:Name="txbAmplitudeIc2"
                                                     MinWidth="80"
                                                     Margin="0,10,10,10"
                                                     Padding="5"
                                                     Text="{Binding ElementName=txbAmplitudeIc, Path=Text}" />
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Margin="10,10,0,10"
                                                       Padding="5"
                                                       Text="IPa:"
                                                       VerticalAlignment="Center"
                                                       HorizontalAlignment="Center" />
                                            <TextBox x:Name="txbAmplitudeIPa2"
                                                     MinWidth="80"
                                                     Margin="0,10,10,10"
                                                     Padding="5"
                                                     Text="{Binding ElementName=txbAmplitudeIPa, Path=Text}" />
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Margin="10,10,0,10"
                                                       Padding="5"
                                                       Text="IPb:"
                                                       VerticalAlignment="Center"
                                                       HorizontalAlignment="Center" />
                                            <TextBox x:Name="txbAmplitudeIPb2"
                                                     MinWidth="80"
                                                     Margin="0,10,10,10"
                                                     Padding="5"
                                                     Text="{Binding ElementName=txbAmplitudeIPb, Path=Text}" />
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Margin="10,10,0,10"
                                                       Padding="5"
                                                       Text="IPc:"
                                                       VerticalAlignment="Center"
                                                       HorizontalAlignment="Center" />
                                            <TextBox x:Name="txbAmplitudeIPc2"
                                                     MinWidth="80"
                                                     Margin="0,10,10,10"
                                                     Padding="5"
                                                     Text="{Binding ElementName=txbAmplitudeIPc, Path=Text}" />
                                        </StackPanel>
                                    </UniformGrid>
                                </Grid>
                            </GroupBox>
                        </Grid>
                    </GroupBox>
                    <TextBox x:Name="tbxReadACSourceDataResponse"
                             Margin="10,5"
                             Padding="10"
                             Background="AliceBlue"
                             TextWrapping="WrapWithOverflow"
                             ScrollViewer.CanContentScroll="True"
                             VerticalScrollBarVisibility="Auto"
                             Grid.Row="1"
                             Grid.Column="0"
                             Grid.ColumnSpan="2" />
                </Grid>
            </TabItem>
            <!--#endregion 交流源/表:读取数据-->
            <!--#region 直流表-->
            <TabItem Header="直流表"
                     Padding="10"></TabItem>
            <!--#endregion 直流表-->
            <!--#region 直流源/电阻-->
            <TabItem Header="直流源/电阻"
                     Padding="10"></TabItem>
            <!--#endregion 直流源/电阻-->
            <!--#region 校准-->
            <TabItem Header="校准"
                     Padding="10"></TabItem>
            <!--#endregion 校准-->
            <!--#region 带时间控制的标准源-->
            <TabItem Header="带时间控制的标准源"
                     Padding="10"></TabItem>
            <!--#endregion 带时间控制的标准源-->
            <!--#region 遥信控制命令-->
            <TabItem Header="遥信控制命令"
                     Padding="10"></TabItem>
            <!--#endregion 遥信控制命令-->
            <!--#region DK-51F 的参数校准规程-->
            <TabItem Header="DK-51F 的参数校准规程"
                     Padding="10"></TabItem>
            <!--#endregion DK-51F 的参数校准规程-->
        </TabControl>
    </Grid>
</Window>
