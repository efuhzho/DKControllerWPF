                                               Margin="10,5,0,5"
                                               VerticalAlignment="Center" />
                                    <TextBox Name="ReadACSourceRangesErrorCode"
                                             VerticalContentAlignment="Center"
                                             Margin="5"
                                             Padding="5" />
                                    <TextBlock Text="Message:"
                                               Margin="10,5,0,5"
                                               VerticalAlignment="Center" />
                                    <TextBox Name="ReadACSourceRangesMesseage"
                                             VerticalContentAlignment="Center"
                                             Margin="5"
                                             Padding="5" />
                                    <Label Content="ACU Ranges:"
                                           VerticalContentAlignment="Center"
                                           Margin="10,5,0,5" />
                                    <ComboBox x:Name="cbxACURangs"
                                              Margin="5"
                                              Padding="5"
                                              SelectedIndex="0"
                                              IsReadOnly="True" />
                                    <Label Content="ACI Ranges:"
                                           VerticalContentAlignment="Center"
                                           Margin="10,5,0,5" />
